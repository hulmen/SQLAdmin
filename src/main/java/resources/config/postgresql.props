INTEGER=INTEGER
BIGINT=BIGINT
REAL=REAL
DOUBLE=DOUBLE PRECISION
NUMERIC=DECIMAL
CHAR=CHAR
VARCHAR=VARCHAR
LONGVARCHAR=TEXT
DATE=DATE
TIME=TIME
TIMESTAMP=TIMESTAMP
CLOB=TEXT
BLOB=BYTEA
ARRAY=ARRAY
